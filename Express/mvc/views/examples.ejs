<%- include('partials/header.ejs'); %>

    <div id="examples">

        <h1>Examples</h1>

        <p><%= body %></p>

        <% if( locals.backend && backend.length > 0 ) { %>

            <h2>Backend</h2>
            <ol type="I">
                <% for(let i = 0; i < backend.length; i++) { %>
                    <li><%= backend[i] %></li>
                <% } %>
            </ol>
        <% } %>

        <% if( locals.frontend ) { %>
            <h2>Front End</h2>

            <ol type="I">
                <li><%= frontend.markup || "HTML" %></li>
                <li><%= frontend.styles || "CSS" %></li>
                <li><%= frontend.scripts || "JS" %></li>

                <li>
                    Frameworks
                    <ol>
                        <li><%= frontend.frameworks.css || "No CSS Framework" %></li>
                        <li><%= frontend.frameworks.spa || "No SPA Framework" %></li>
                    </ol>
                </li>
            </ol>
        <% } %>

    </div>

<%- include('partials/footer.ejs'); %>